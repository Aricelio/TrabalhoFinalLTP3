	<queryString>
		<![CDATA[SELECT 
date(v.data),v.valorTotal,p.nome as cliente,pe.nome as funcionario, v.formaPagamento
FROM vendas v 
join clientes c on v.codCliente = c.codCliente
join pessoas p on p.codPessoa = c.codCliente
join funcionarios f on v.codFuncionario = f.codFuncionario
join pessoas pe on pe.codPessoa = f.codFuncionario

where v.formaPagamento like "Á prazo"]]>
	</queryString>
